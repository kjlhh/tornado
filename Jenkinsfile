pipeline {
  agent any
  stages {
    stage('error') {
      steps {
        echo 'test2 done'
      }
    }
    stage('check host') {
      steps {
        echo 'check host done'
      }
    }
    stage('some check') {
      steps {
        echo 'some check'
      }
    }
    stage('done') {
      steps {
        echo 'done'
      }
    }
  }
}